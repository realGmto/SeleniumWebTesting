<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Testing Swag lab">
    <test name="Login-chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="Login.UserLoginTests"/>
            <class name="Login.AccessWithoutLoginTests"></class>
        </classes>
    </test>

    <test name="Main-chrome">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="MainPage.ListProductsTests"></class>
        </classes>
    </test>



    <test name="Login-edge">
        <parameter name="browser" value="edge"></parameter>
        <classes>
            <class name="Login.UserLoginTests"/>
            <class name="Login.AccessWithoutLoginTests"></class>
        </classes>
    </test>

    <test name="Main-edge">
        <parameter name="browser" value="edge"></parameter>
        <classes>
            <class name="MainPage.ListProductsTests"/>
        </classes>
    </test>
</suite> <!-- Suite -->